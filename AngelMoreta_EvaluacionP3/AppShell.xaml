<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="AngelMoreta_EvaluacionP3.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:AngelMoreta_EvaluacionP3"
    xmlns:views="clr-namespace:AngelMoreta_EvaluacionP3.Views"
    Shell.FlyoutBehavior="Flyout">

    <FlyoutItem Title="Lista de API"
                Icon="">
        <Tab>
            <ShellContent ContentTemplate="{DataTemplate views:AM_FraseListaAPI}" />
        </Tab>
    </FlyoutItem>
    <FlyoutItem Title="Lista de Base de Datos"
                Icon="">
        <Tab>
            <ShellContent ContentTemplate="{DataTemplate views:AM_FraseListaDB}" />
        </Tab>
    </FlyoutItem>

</Shell>
